<div class="row d-none d-xl-flex">
    <div class="col-10">
        <h1>Stream</h1>
    </div>
    <div class="col-2">
        <h1>Vid√©os</h1>
    </div>
</div>

<div class="row">
    <div class="col-12 col-xl-10 twitch mb-5 mb-xl-0">
        <div class="twitch-video">
            <iframe frameborder="0" scrolling="no" allowfullscreen="true" height="100%" width="100%" title="stream"
                src="https://player.twitch.tv/?channel=nemaidestft&parent=nemaides.fr&parent=www.nemaides.fr&parent=localhost">
            </iframe>
        </div>
        <div class="twitch-chat">
            <iframe frameborder="0" scrolling="no" height="100%" width="100%" title="chat"
                src="https://www.twitch.tv/embed/nemaidestft/chat?darkpopout&parent=nemaides.fr&parent=www.nemaides.fr&parent=localhost">
            </iframe>
        </div>
    </div>
    <div class="col-12 col-xl-2 youtube">
        <% for(const id of videoIds){ %>
            <div class="youtube-video <%=id != videoIds[3] ? "mb-1" : ""%>">
                <iframe height="100%" width="100%" src="https://www.youtube.com/embed/<%=id%>" title="YouTube video player" frameborder="0" 
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen>
                </iframe>
            </div>
        <% } %>
    </div>
</div>